use AppleScript version "2.4" -- Yosemite (10.10) or lateruse scripting additionstell application "Typora"	set Typora_selection to selection	set wikiLinks to {}	set the clipboard to ""	repeat with i from 1 to the count of Typora_selection		set NoteURL to notelink of item i of Typora_selection		set noteName to title of item i of Typora_selection		set theWikiLink to "[" & NoteURL & " " & noteName & "]<br />"		set the clipboard to (the clipboard) & theWikiLink & return	end repeatend tell