use AppleScript version "2.4" -- Yosemite (10.10) or lateruse scripting additionsset list_Tabs to {}tell application "Safari"	set the_Tabs to (tabs of every window)	repeat with the_Tab in the_Tabs		set the_Items to (every item of the_Tab)		repeat with the_Item in the_Items			copy URL of the_Item to end of list_Tabs		end repeat	end repeatend telltell application "Google Chrome"	repeat with list_Tab in list_Tabs		set myTab to make new tab at end of tabs of window 1		set URL of myTab to list_Tab	end repeatend tell